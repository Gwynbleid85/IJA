<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox fx:id="attribute" alignment="CENTER" prefHeight="24.0" prefWidth="590.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ComboBox fx:id="accessMod" promptText="+">
         <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
         </HBox.margin>
      </ComboBox>
      <TextField fx:id="datatype">
         <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
         </HBox.margin>
      </TextField>
      <TextField fx:id="name">
         <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="2.0" />
         </HBox.margin>
      </TextField>
      <Button fx:id="delButton" mnemonicParsing="false" text="X">
         <HBox.margin>
            <Insets left="30.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
