            </Button>
            <Button fx:id="newSeqDia" mnemonicParsing="false" text="New Sequence diagram" />
            <ComboBox fx:id="SetSeqDiagram" promptText="Choose sequence diagram">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
